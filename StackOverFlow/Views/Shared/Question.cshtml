@model StackOverFlow.ViewModels.QuestionViewModel
<tr>
    <td><span class="badge badge-secondary">@Model.VotesCount</span> Votes</td>
    <td><span class="badge badge-secondary">@Model.AnswersCount</span> Answer</td>
    <td><span class="badge badge-secondary">@Model.ViewsCount</span> Views</td>
    <td class="text-dark">
        <a href="/questions/view?questionId=@Model.QuestionID" class="text-primary">@Model.QuestionName</a>
    </td>
    <td><span class="badge badge-info">@Model.Category.CategoryName</span></td>
    <td>
        @if (Convert.ToInt32(Session["CurrentUserID"]) != Model.UserID)
        {
            @:by @Model.User.Name
        }
        else
        {
            @:by you
        }
    </td>
    <td>@Model.User.Name</td>
    <td>
        @Model.QuestionDateAndTime
    </td>
</tr>